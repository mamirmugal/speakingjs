{"ts":1373897145011,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"\n/**\n * Getting param from the url\n */\nfunction getRequest(val)\n{  \n    val = val.replace(/[\\[]/,\"\\\\\\[\").replace(/[\\]]/,\"\\\\\\]\");  \n    var rgx = \"[\\\\?&]\"+val+\"=([^&#]*)\";  \n    var regex = new RegExp( rgx );  \n    var results = regex.exec(window.location.href);\n    if(results == null)\n        return \"\";  \n    else    \n        return results[1];\n}\n\n/**\n * Getting Request form the url\n */\nfunction getUri()\n{\n    return getRequest(\"act\");\n}\n"]],"start1":0,"start2":0,"length1":0,"length2":442}]],"length":442}
